import entities.Rent;

public class Program {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		Rent rr = new Rent("Maria","maria@exemplo.com");
		
		
		System.out.println(rr);
		sc.close();

	}

}
